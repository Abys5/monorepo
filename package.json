{
  "name": "doggo-sports",
  "version": "0.0.0",
  "author": "Anthony Shew",
  "scripts": {
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "dev:build": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dev:kill": "docker-compose down",
    "test:build": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --build",
